{
  "name": "chatter",
  "version": "1.0.0",
  "description": "online chat room app",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "npm server.js",
    "server": "nodemon server.js --ignore 'accounts.json'",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Zach Dobbs",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "react-materialize": "^2.4.8",
    "socket.io": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  }
}
